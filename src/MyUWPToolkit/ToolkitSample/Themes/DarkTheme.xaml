<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    >
    <!--Common-->
    <SolidColorBrush x:Key="CustomForegroundBrush">#FFFFFFFF</SolidColorBrush>
    <SolidColorBrush x:Key="CustomBackgroundBrush">#FF000000</SolidColorBrush>
    <SolidColorBrush x:Key="CustomForegroundRedBrush">#FFFF0000</SolidColorBrush>
    <SolidColorBrush x:Key="CustomForegroundGreenBrush">#FF01E921</SolidColorBrush>
    <SolidColorBrush x:Key="CustomForegroundBlueBrush">#FF71B4FF</SolidColorBrush>
    <SolidColorBrush x:Key="CustomForegroundYellowBrush">#FFFF9500</SolidColorBrush>
    <SolidColorBrush x:Key="CustomSectionTitleForegroundBrush" Color="#FFB5B5B5"/>
    <SolidColorBrush x:Key="CustomHighlightForegroundThemeBrush">#FF3381E3</SolidColorBrush>
    <SolidColorBrush x:Key="CustomSecondaryForegroundThemeBrush">#FF999999</SolidColorBrush>
    <SolidColorBrush x:Key="CustomSeparatorThemeBrush">#FF333333</SolidColorBrush>
    <SolidColorBrush x:Key="GridBackground">#1A1A1A</SolidColorBrush>
    <!--Common-->
    <!--CustomKeyboard-->
    <SolidColorBrush x:Key="keyboardButtonForeground" Color="#cccccc"/>
    <SolidColorBrush x:Key="keyboardLightButtonBackground" Color="#444444"/>
    <SolidColorBrush x:Key="KeyboardDeepButtonBackground" Color="#2e2e2e"/>
    <SolidColorBrush x:Key="keyboardBackground" Color="#202020"/>
    <!--CustomKeyboard-->
</ResourceDictionary>
